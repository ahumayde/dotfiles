{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#18354c",
          "foreground": "#F9F1A5",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        }, {
          "type": "git",
          "style": "diamond",
          "foreground": "#7b44ff",
          "background": "#18354c",
          "template": "{{ .UpstreamIcon }} {{ .HEAD }}",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "properties": { "fetch_upstream_icon": true }
        }, {
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
          "{{ if gt .Behind 0 }}#D22A24{{ end }}",
          "{{ if gt .Ahead  0 }}#29FCBC{{ end }}"],
          "background": "#18354c",
          "template": "{{if or (gt .Ahead 0) (gt .Behind 0) }}{{ .BranchStatus }}{{ end }}",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "properties": { "fetch_status": true }
        }, {
          "type": "git",
          "style": "diamond",
          "foreground": "#00ff00",
          "background": "#18354c",
          "template": "{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}",
          "leading_diamond": "\ue0b6",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "properties": { "fetch_status": true }
        }, {
          "type": "git",
          "style": "diamond",
          "foreground": "#ff2222",
          "background": "#18354c",
          "template": "{{ if .Working.Changed }} {{ .Working.String }}{{ end }}",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "properties": { "fetch_status": true }
        }, {
          "type": "git",
          "style": "diamond",
          "foreground": "#D75CFF",
          "background": "#18354c",
          "template": "{{ if gt .StashCount 0 }}\ueb4b {{ .StashCount }}{{ end }}",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "properties": { "fetch_stash_count": true }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#18354c",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "properties": {
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          },
          "style": "diamond",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 <#262626></>",
          "type": "battery"
        },
        {
          "background": "#18354c",
          "foreground": "#F9F1A5",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "<#002030,#18354c>\ue0b2",
          "properties": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} </>",
          "type": "os"
        },
        {
          "background": "#18354c",
          "foreground": "#F9F1A5",
          "leading_diamond": "<#18354c,#002030>\ue0b2",
          "properties": {
            "time_format": "_2,15:04"
          },
          "style": "diamond",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#e0f8ff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
